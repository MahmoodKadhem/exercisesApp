{
  "name": "exercisesapp",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "prefix:css": "postcss public/css/style.comp.css -u autoprefixer -b 'last 10 versions' -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "live-server": "^1.2.1",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^8.3.1"
  },
  "author": "MahmoodKadhem",
  "license": "ISC"
}
